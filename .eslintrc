{
  "parser": "babel-eslint",
  "extends": "eslint-config-airbnb",
  "rules": {
      // Allow
      "arrow-body-style": 0,
      "object-shorthand": 0,
      "max-len": 0,
      "no-underscore-dangle": 0,
      "class-methods-use-this": 0,
      // React
      "react/prop-types": 0,
      "react/self-closing-comp": 0,
      "react/jsx-filename-extension": 0,
      "react/prefer-stateless-function": 0
    }
}
